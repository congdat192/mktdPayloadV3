{
  "name": "payload-cms-backend",
  "version": "1.0.0",
  "description": "Payload CMS backend for WordPress migration",
  "main": "dist/server.js",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts NODE_OPTIONS='--no-deprecation' ts-node src/server.ts",
    "build": "tsc",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.654.0",
    "@aws-sdk/lib-storage": "^3.654.0",
    "@payloadcms/bundler-webpack": "^1.0.7",
    "@payloadcms/db-postgres": "^0.8.0",
    "@payloadcms/richtext-lexical": "^0.11.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "payload": "^2.28.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.9",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}